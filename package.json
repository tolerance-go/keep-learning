{
  "name": "keep-learning",
  "repository": "git@github.com:tolerance-go/keep-learning.git",
  "author": "yarnb <yarnbcoder@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:components": "start-storybook",
    "run:server": "nodemon --watch 'service/**/*' -e ts --exec 'ts-node' --project tsconfig.server.json ./service/index.ts",
    "run:as-app": "parcel AnswerSheet/index.html",
    "run:as-server": "nodemon --watch 'AnswerSheet-service/**/*' -e ts --exec 'ts-node' --project tsconfig.as-server.json ./AnswerSheet-service/index.ts",
    "build": "node ./scripts/generateReadme.js",
    "build:components": "build-storybook -o dist-components",
    "build:as-app": "parcel build -d dist-as-app AnswerSheet/index.html",
    "build:as-server": "tsc --build tsconfig.as-server.json",
    "build:server": "tsc --build tsconfig.server.json",
    "publish": "npm test && npm run build && lerna-changelog"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts?(x),js}": [
      "eslint --fix",
      "prettier --parser=typescript --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run build",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "changelog": {
    "labels": {
      "enhancement": ":rocket: Enhancement",
      "bug": ":bug: Bug Fix",
      "documentation": ":book: Documentation",
      "dependency": ":deciduous_tree: Dependency",
      "chore": ":turtle: Chore"
    }
  },
  "dependencies": {
    "koa": "^2.11.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "shelljs": "^0.8.3",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@storybook/addon-a11y": "^5.3.17",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-storyshots": "^5.3.17",
    "@storybook/addon-storysource": "^5.3.17",
    "@storybook/addon-viewport": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "@types/jest": "^25.2.1",
    "@types/koa": "^2.11.3",
    "@types/react-dom": "^16.9.6",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lerna-changelog": "^1.0.1",
    "lint-staged": "^10.1.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-test-renderer": "^16.13.1",
    "ts-loader": "^7.0.0",
    "ts-node": "^8.9.0",
    "typescript": "~3.7.2"
  }
}
